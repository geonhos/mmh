{
  "plansDirectory": "./plans",
  "permissions": {
    "deny": [
      "Bash(sudo:*)",
      "Bash(chmod 777:*)",
      "Read(./.env)",
      "Read(./.env.*)",
      "Read(./secrets/**)",
      "Read(**/*credentials*)"
    ]
  },
  "extraKnownMarketplaces": {
    "geonhos-plugins": {
      "source": {
        "source": "github",
        "repo": "geonhos/claude-code-setup"
      }
    }
  },
  "enabledPlugins": {
    "multi-agent-system@geonhos-plugins": true
  }
}
